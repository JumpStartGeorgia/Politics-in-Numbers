#explore {
  position:relative;
  .filter-type {
    background-color:#F5F7F5;
    width:100%;
    padding:60px 0 28px;
    margin:0px auto;
    > div {
      display:table-cell;
      vertical-align:top;
    }
    .back {
      display:none;
      img {
      }
      cursor:pointer;
    }
    .sep {
      height:100%;
      border-left:1px dashed #a9afa8;
    }
    .donation {
      width:50%;
      text-align:right;
      padding:8px 26px 8px 0;
      .donation-toggle {
        font: 16px firasans_sb;
        color:#DDCD37;
        background-color:transparent;
        padding:0 11px;
        border:3px solid #DDCD37;
        height:56px;
        min-width:111px;
        transition-duration: 0.3s;
        transition-property: color, background-color;
        position: relative;
        top: -1px;
        left: -1px;
        &.active, &:hover{
          background-color: #DDCD37;
          color:#3E443D;
          cursor:pointer;
        }
      }
      &.active {
        .donation-toggle {
          background-color: #DDCD37;
          color:#3E443D;
        }
      }
    }

    .finance {
      width:50%;
      text-align:left;
      padding:8px 0 8px 26px;
      .finance-toggle {
        float:left;
        font: 16px firasans_sb;
        color:#F78E69;
        text-align:left;
        background-color:transparent;
        padding:0 11px;
        border:3px solid #F78E69;
        height:56px;
        min-width:111px;
        transition-duration: 0.3s;
        transition-property: color, background-color;
        vertical-align:top;
        &:hover {
          background-color: #F78E69;
          color:#fff;
          cursor:pointer;
        }
      }
      &.active {
        .finance-container {
          padding:0 0 0 20px;
        }
        .finance-toggle {
          background-color: #F78E69;
          color:#fff;
          position: relative;
          top: -1px;
          left: -21px;
        }
      }
      #finance_category {
        display:none;
        li {
          display:inline-block;
          vertical-align:top;
          padding:9px 0 10px;
          .finance-category-toggle {
            display:inline-block;

            opacity:.6;
            height:37px;
            min-width:90px;

            border:1px solid #9aa199;
            position:relative;
            &[data-state='']:hover a.label:before,
            &[data-state='']:focus a.label:before,
            &[data-state='simple']:hover a.label:before,
            &[data-state='simple']:focus a.label:before {
              content:"";
              display:inline-block;
              background:rgba(136,216,119,.8) url("<%= asset_path('explore/add.svg') %>") no-repeat center;
              background-size:20px;
              position:absolute;
              top:0px;
              right:0px;
              bottom:0px;
              left:0px;
            }
            a.label {
              font:12px firasans_sb;
              color:#5D675B;
              text-decoration:none;
              display: inline-block;
              padding:11px 9px 0;
              width: 100%;
              height: 100%;
              vertical-align: middle;
              span { display:none; }
            }
            a.label span {
              font:12px firasans_r;
              color:#fff;
              padding:4px 0 0;
            }
            a.label.long {
              padding:4px 9px 0;
            }
            a.sub {
              display:none;
              font:12px firasans_book;
              color:#5D675B;
            }

            &[data-state='preselect'] {
              border:1px dashed #9aa199;
              a.label { padding:0; }
              a.sub {
                padding:2px 9px 0;
                display:block;
                height:50%;
                position:relative;
                &:hover:before {
                  content:"";
                  display:inline-block;
                  background:rgba(136,216,119,.8) url("<%= asset_path('explore/add.svg') %>") no-repeat center;
                  background-size:13px;
                  position:absolute;
                  top:0px;
                  right:0px;
                  bottom:0px;
                  left:0px;
                }
              }
              a.sub[data-sub='all'] {
                border-bottom:1px dashed #9aa199;
              }
              a.label {
                display:none;
              }
            }
            &[data-state='all'] {
              background-color:#5D675B;
              a.label {
                color:#fff;
                padding:0 9px;
                span.f { display:block; }
              }
            }
            &[data-state='campaign'] {
              background-color:#5D675B;
              a.label {
                color:#fff;
                padding:0 9px;
                span.s { display:block; }
              }
            }
            &[data-state='simpled'] {
              background-color:#5D675B;
              a.label {
                color:#fff;
              }
            }
            &[data-state='all']:hover,
            &[data-state='campaign']:hover,
            &[data-state='simpled']:hover {
              a.label:before {
                content:"";
                display:inline-block;
                background:rgba(247,142,105,.8) url("<%= asset_path('explore/remove.svg') %>") no-repeat center;
                background-size:20px;
                position:absolute;
                top:0px;
                right:0px;
                bottom:0px;
                left:0px;
              }
            }


          }
          &:first-of-type {
            a {
              border-left:0;
            }
          }
        }
      }
    }
    &[data-type='donation'] {
      // .donation {
      //   width:100%;
      //   text-align:center;
      // }
      // .finance {
      //   display:none;
      // }
      // .sep {
      //   display:none;
      // }
      // .forward {
      //   display:table-cell;
      //   padding: 7px 0 9px;
      // }
    }
    &.in-depth[data-type='finance'] {
      .donation {
        display:none;
      }
      .finance {
        width:100%;
        #finance_category {
          display:inline;
          margin-left: -21px;
        }
        .finance-container {
          border:1px dashed #ced1cd;
          border-right:0;
        }
      }
      .sep {
        display:none;
      }
      .back {
        display:table-cell;
        padding: 7px 0 6px;
      }
    }
  }
  #finance_chart {
    .highcharts-container {
      margin:0 auto;
    }
  }
  .filter-extended {
    position:relative;
    display:inline-block;
    width:100%;
    .filter-toggle {
      font:14px firasans_r;
      color:#88D877;
      padding:15px 24px 12px 24px;
      border-bottom:1px solid #D7DDD4;
      cursor:pointer;
    }
    .filter-header {
      display:block;
      padding:15px 0 12px;
      border-bottom:1px solid #D7DDD4;
      label {
        display:inline-block;
        font:14px firasans_r;
        color:#88D877;
        padding:0 10px;
      }
      .close {
        float: right;
        vertical-align: middle;
        background:transparent url("<%= asset_path('explore/close.svg') %>") no-repeat center;
        width:20px;
        height:20px;
        position: relative;
        top: -3px;
        cursor:pointer;
      }
    }
    .filter-footer {
      .reset {
        display:block;
        font:14px firasans_b;
        color:#5D675B;
        background-color:#fff;
        border:1px solid #C5CEC2;
        padding:8px 18px;
        margin: 0px auto;
      }
      .explore-button {
        display:block;
        font:14px firasans_b;
        color:#fff;
        background-color:#F78E69;
        padding:8px 18px;
        border:0;
        margin:12px auto 0;
        i.explore {
          display:inline-block;
          background:transparent url("<%= asset_path('explore/compare.svg') %>") no-repeat center;
          width:20px;
          height:20px;
          vertical-align: middle;
          margin-right: 10px;
        }
      }
    }
    .pane {
      display:none;
      background-color:#F5F7F5;
      position:absolute;
      top: 0px;
      z-index:55;
      width:60%;
      min-width:200px;
      padding:0px 14px 12px;
      overflow-y:auto;
      &::-webkit-scrollbar-track {
        background-color: #F5F7F5;
      }
      &::-webkit-scrollbar {
        height: 8px;
        width:8px;
      }
      &::-webkit-scrollbar-thumb {
        background-color:#848484;
        border: 2px solid #F5F7F5;
      }
      &::-webkit-scrollbar-thumb:hover {
        background-color: #848484;
        cursor: pointer;
      }
      .row {
        padding:15px 2px;
      }
      .row:not(:last-of-type) {
        border-bottom:1px solid #D7DDD4;
      }
      // .row:last-of-type {
      //   text-align:center;
      // }
      // .filter-inputs.donation {
      //   display:none;
      // }
      // .filter-inputs.finance {
      //   display:none;
      // }
      .filter-input {
        &[data-on='false'] {
          display:none;
        }
        .toggle {
          position:relative;
          display: inline-block;
          font:14px firasans_l;
          color:#5D675B;
          text-align:left;
          width: 100%;
          padding-right: 16px;
        }
        .sublabel {
          font:12px firasans_r;
          color:#5D675B;
          margin:15px 0 8px;
        }
        .toggle i {
          background:transparent url("<%= asset_path('explore/expand.svg') %>") no-repeat center;
          float:right;
          width:16px;
          height:16px;
          cursor:pointer;
          position:absolute;
          top:0px;
          right:0px;
        }
        &.expanded {
          .toggle i{
            background:transparent url("<%= asset_path('explore/collapse.svg') %>") no-repeat center;
          }
          .input-group {
            display: inline-block;
          }
        }

        .list {
          &.hidden {
            display:none;
          }
          li, span {
            display:inline-block;
            width:100%;
            font:14px firasans_book;
            color:#F78E69;
            padding:4px 14px 4px 0;
            position:relative;
            text-align:left;
            &:first-of-type { margin:12px 0 0; }
            i {
              display: inline-block;
              width: 10px;
              height: 10px;
              background: transparent url("<%= asset_path('explore/close_s.svg') %>") no-repeat right top;
              cursor:pointer;
            }
          }
        }
        .input-group {
          display:none;
          margin:8px 0 0;
          width: 100%;
          input {
            font:12px firasans_r;
            color:#5D675B;
            background-color:#F5F7F5;
            padding:6px 10px;
            width:100%;
            border:1px solid #F78E69;
            outline:none;
            &:focus, &:hover {
              background-color:#fff;
            }
            &.datepicker {
              max-width:100px;
            }
            &[type='checkbox'] {
              width:auto;
            }
            &[type='checkbox'] {
              border:1px solid #F78E69;
              border-radius:0;
            }
          }
          ul {
            margin-bottom:8px;
            li {
              padding:2px 0px;
              a {
                font:14px firasans_book;
                color:#F78E69;
              }
            }
          }
          .block {
            display:inline-block;
            &.b1 {
              margin-bottom:10px;
            }
          }
        }

        .input-radio-group {
          text-align:center;
          input[type="radio"] + label {
            font:12px firasans_r;
            color:#5D675B;
            display:inline-block;
            margin-right:0px;
            padding:0 10px;
          }
          input[type="radio"]:checked + label {
            font:12px firasans_b;
          }
          .or {
            font:14px firasans_book;
            color:#F78E69;
          }
        }
        .input-checkbox-group {
          margin-top:12px;
          ul {
            text-align: left;
            li {
              display:block;
              padding:4px 0;
            }
          }
        }
      }
    }
    &.active[data-type='donation'] {
      .pane { display:inline-block; }
      .donation { display:initial; }
      .finance { display:none; }
    }
    &.active[data-type='finance'] {
      .pane { display:inline-block; }
      .donation { display:none; }
      .finance { display:initial; }
    }
    i.filter {
      display: inline-block;
      background:transparent url("<%= asset_path('explore/filter.svg') %>") no-repeat center;
      vertical-align: middle;
      width:20px;
      height:15px;
      margin-right:12px;
    }
    .autocomplete {
      position:relative;
      input {
        height:32px;
        &:hover, &:focus {
          background-color:#fff;
        }
      }
      .dropdown {
        display:none;
        position:absolute;
        z-index:66;
        top:100%;
        left:0;
        bottom:-500%;
        background-color:#fff;
        border: 1px solid #F78E69;
        border-top:0;
        max-height:700%;
        overflow-y:auto;
        width:100%;
        > li {
          .item { padding:8px 10px; }
          li {
            .item { padding:8px 2px; }
          }
        }
        li {
          font:14px firasans_book;
          color:#F78E69;
          cursor:pointer;
          .item {
            text-align:left;
            width:100%;
            &:hover {
              background-color: #F5F7F5;
            }

          }
          &.l1 .item { padding-left:23px; }
          &.l2 .item { padding-left:26px; }
          &.l3 .item { padding-left:29px; }
          &.l4 .item { padding-left:32px; }
          &.l5 .item { padding-left:35px; }
          &.l1 .tree-toggle { padding-left:3px; }
          &.l2 .tree-toggle { padding-left:6px; }
          &.l3 .tree-toggle { padding-left:9px; }
          &.l4 .tree-toggle { padding-left:12px; }
          &.l5 .tree-toggle { padding-left:15px; }
          ul { display:none; }
          // outline:none;
          &.collapse {
            > .tree-toggle {
              position:relative;
              display: inline-block;
              padding-right: 16px;
              background:transparent url("<%= asset_path('explore/expand.svg') %>") no-repeat center;
              width:16px;
              height:32px;
              cursor:pointer;
              float:left;
            }
          }
          &.expanded {
            > .tree-toggle {
              background:transparent url("<%= asset_path('explore/collapse.svg') %>") no-repeat center;
            }
            > ul { display: inline-block; }
          }

          // &.expanded {
          //   .toggle i{
          //     background:transparent url("<%= asset_path('explore/collapse.svg') %>") no-repeat center;
          //   }
          //   .input-group {
          //     display: inline-block;
          //   }
          // }
        }
        &::-webkit-scrollbar-track {
          background-color: #F5F7F5;
        }
        &::-webkit-scrollbar {
          height: 8px;
          width:8px;
        }
        &::-webkit-scrollbar-thumb {
          background-color:#848484;
          border: 2px solid #F5F7F5;
        }
        &::-webkit-scrollbar-thumb:hover {
          background-color: #848484;
          cursor: pointer;
        }
      }
      &.active {
        input {
          background-color:#fff !important;
        }
        .dropdown {
          display:initial;
          .selected, .focus {
            background-color: #F5F7F5;
          }
        }
      }
    }
  }
  .content {
    position:relative;
    display:inline-block;
    vertical-align:top;
    // padding:10px;
    width:100%;
    .panes {
      .pane[data-type='donation'] {
        .caption {
          font:18px firasans_r;
          color:#5d675b;
          text-align:center;
          padding:20px 10px 15px;
        }
        .charts {
          .chart-container {
            display:block;
            width:100%;
            min-width:300px;
            padding:12px 6px 22px;
            .actions {
              text-align:center;
              padding:0 0 18px;
              > div
              {
                display:inline-block;
                position:relative;
                width:36px;
                height:28px;
                > i {
                  display:block;
                  width:24px;
                  height:21px;
                  position:absolute;
                  top:50%;
                  left:50%;
                  transform:translate(-50%, -50%);
                  background:transparent url("<%= asset_path('explore/actions.svg') %>") no-repeat top left;
                  &:hover, &:focus, &:active {
                    cursor:pointer;
                  }
                  margin:0px auto;
                }
              }
              .share
              {
                i { background-position: 0px 0px; }
                &:hover { background-position: 0px -22px; }
              }
              .embed
              {
                i { background-position: -25px 0px; }
                &:hover i { background-position: -25px -22px; }
              }
              .download{
                i { background-position: -49px 0px; }
                &:hover i { background-position: -49px -22px; }
              }
            }
          }
          .chart-container-1 {
            background-color:#ebe187;
          }
          .separator {
            display:none;
          }
          .chart-container-2 {
            background-color:#b8e8ad;
          }
        }
        .total {

          margin:20px 0 42px;
          > div {
            display:table-cell;
            &:not(.separator) {

              width:50%;
              text-align:center;
            }
          }
          .box {
            padding:12px 24px;
            margin:10px 0;
            background-color:#F5F7F5;
          }
          label {
            display:block;
            font:14px firasans_r;
            margin-bottom:8px;
            color:#C1B02F;
          }
          span {
            display:block;
            font:16px firasans_b;
            color:#5D675B;
          }
          .separator {
            min-width: 55px;
            height:80px;
            background:#F78E69 url("<%= asset_path('explore/stats.svg') %>") no-repeat center;
            background-size:33px;
            position: relative;
            &:after {
              content: "";
              width: 0;
              height: 0;
              border-style: solid;
              border-width: 14px 27px 0 27px;
              border-color: #F78E69 transparent transparent transparent;
              line-height: 0px;
              _border-color: #F78E69 #000000 #000000 #000000;
              _filter: progid:DXImageTransform.Microsoft.Chroma(color='#000000');
              position: absolute;
              bottom: -14px;
            }
          }
        }
        .table {
          .actions {
            text-align:right;
            padding:0 16px 18px;
            > div, > a
              {
                display:inline-block;
                position:relative;
                width:36px;
                height:28px;
                > i {
                  display:block;
                  width:24px;
                  height:21px;
                  position:absolute;
                  top:50%;
                  left:50%;
                  transform:translate(-50%, -50%);
                  background:transparent url("<%= asset_path('explore/actions.svg') %>") no-repeat top left;
                  &:hover, &:focus, &:active {
                    cursor:pointer;
                  }
                  margin:0px auto;
                }
              }

            .share {
              i { background-position: 0px -43px; }
              &:hover i { background-position: 0px -65px; }
            }
            .embed {
              i { background-position: -25px -43px; }
              &:hover i { background-position: -25px -65px; }
            }
            .download {
              i { background-position: -49px -43px; }
              &:hover i { background-position: -49px -65px; }
            }
          }
          #donation_table {
            width:100%;
            padding:0px 6px;
            table {
              width:100%;
              thead {
                border:1px solid #c5cec2;
                background-color:#f3f9f2;
                // e2e6e0
                //
                th {
                  font:16px firasans_r;
                  color:#5D675B;
                  text-align:left;
                  padding:15px 13px;
                  &:not(:last-of-type) {
                    border-right:1px solid #e2e6e0;
                  }
                }
              }
              tbody {
                tr {
                  border:1px solid #e2e6e0;
                  border-top:0;
                  td {
                    font:14px firasans_l;
                    color:#5D675B;
                    text-align:left;
                    padding:15px 13px;
                    vertical-align:top;
                    &:not(:last-of-type) {
                      border-right:1px solid #e2e6e0;
                    }
                    button.view {
                      font:12px firasans_r;
                      color:#383d37;
                      background-color: #ddcd37;
                      padding:9px 12px;
                      border:0;
                      float:right;
                      &:hover, &:focus, &:active {
                        text-decoration:none;
                        color:#383d37;
                      }
                    }
                    &.right { text-align: right; }
                    &.center { text-align: center; }
                  }

                }
              }
            }
          }
        }
      }
      .pane[data-type='finance'] {
        .actions {
          padding:26px 59px 0;
          .left {
            label {
              display: inline-block;
              font:14px firasans_l;
              color: #5D675B;
              vertical-align: top;
              margin:10px 0 8px 0;
            }
            > div[data-view-toggle] {
              display:inline-block;
              width: 40px;
              height: 36px;
              padding:8px 10px;
              cursor:pointer;
              border:1px dashed transparent;
              &:hover, &:focus, &:active {
                border:1px dashed #AEB3AD;
              }

            }
            > div[data-view-toggle='chart'] {
              background:transparent url("<%= asset_path('explore/chart.svg') %>") no-repeat center/ 20px;
            }
            > div[data-view-toggle='table'] {
              background:transparent url("<%= asset_path('explore/table.svg') %>") no-repeat center/ 20px;
            }
          }
        }
        [data-view] {
          display:none;
        }
        &[data-view-current='chart'] {
          .actions .left [data-view-toggle='chart'] {
            border:1px dashed #AEB3AD;
            background-image:url("<%= asset_path('explore/chart_a.svg') %>");
          }
          [data-view='chart'] {
            display:initial;
          }
        }
        &[data-view-current='table'] {
          .actions .left [data-view-toggle='table'] {
            border:1px dashed #AEB3AD;
            background-image:url("<%= asset_path('explore/table_a.svg') %>");
          }
          [data-view='table'] {
            display:initial;
          }
        }
        table {
          width:100%;
          border-bottom:1px solid #DEE2DC;
          thead {
            // border:1px solid #DEE2DC;
            th {
              font:16px firasans_r;
              color:#5D675B;
              text-align:left;
              background-color:#F3F9F2;
              padding:15px 13px;
              border:1px solid #DEE2DC;
              border-left:0;
              border-bottom:0;
              &:first-of-type {
                border-left:1px solid #DEE2DC;
              }
              &.empty {
                background-color:#fff;
                border:0;
              }
              &.outer {
                background-color:#fff;
              }
              &:first-of-type {
                border-right:2px solid #DEE2DC;
              }
              &.center { text-align:center; }
              &.right { text-align:right; }
            }
          }
          tbody {
            tr {
              td {
                font:14px firasans_l;
                color:#5D675B;
                text-align:left;
                padding:15px 13px;
                vertical-align:top;
                background-color:#fff;
                border:1px solid #DEE2DC;
                border-bottom:0;
                border-left:0;
                &:first-of-type {
                  font:16px firasans_r;
                  color:#5D675B;
